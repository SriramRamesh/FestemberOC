<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.festemberoc"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="18"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/festember_logo2"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.android.festemberoc.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.android.festemberoc.QRscanner"
            android:label="@string/title_activity_qrscanner" />
        <activity
            android:name="com.example.android.festemberoc.Success"
            android:label="@string/title_activity_success" />
        <activity
            android:name="com.example.android.festemberoc.Select_female_tshirt"
            android:label="@string/title_activity_seleact_female_tshirt" />
        <activity
            android:name="com.example.android.festemberoc.Select_male_tshirt"
            android:label="@string/title_activity_select_male_tshirt" />
        <activity
            android:name="com.example.android.festemberoc.Select_fCard"
            android:label="@string/title_activity_select_f_card" />
        <activity
            android:name="com.example.android.festemberoc.pinActivity"
            android:label="@string/title_activity_pin" />
    </application>

</manifest>